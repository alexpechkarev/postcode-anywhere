{
  "name": "alexpechkarev/postcode-anywhere",
  "description": "Laravel API wrapper for PCA Predict formerly Postcode Anywhere",
  "keywords": [
    "postcode anywhere",
    "postcode lookup",
    "address lookup",
    "find and retrieve"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Pechkarev",
      "email": "alexpechkarev@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "laravel/framework": "6.*"
  },
  "require-dev": {
    "laravel/framework": "6.*",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "PostcodeAnywhere\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
        "providers": [
            "PostcodeAnywhere\\PAServiceProvider"
        ],
        "aliases": {
            "PA": "PostcodeAnywhere\\PAFacade"
        }
    }
  }
}
